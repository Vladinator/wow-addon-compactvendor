					<Anchor point="RIGHT" relativeKey="$parent.Cost2" relativePoint="LEFT" x="-4" y="0"/>
				</Anchors>
			</Button>
			<Button parentArray="Costs" parentKey="Cost4" inherits="VladsVendorListItemCostButtonTemplate">
